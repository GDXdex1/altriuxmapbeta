    // === GENESIS: Spawn all cities, city-states, and mines ===
    public fun genesis_spawn_all_cities(
        pop_reg: &mut PopulationRegistry,
        mining_reg: &mut MiningRegistry,
        clock: &Clock,
        ctx: &mut TxContext
    ) {
        spawn_tundra_cities(pop_reg, mining_reg, clock, ctx);
        spawn_plains_cities(pop_reg, mining_reg, clock, ctx);
        spawn_meadow_cities(pop_reg, mining_reg, clock, ctx);
        spawn_cordillera_cities(pop_reg, mining_reg, clock, ctx);
        spawn_desert_cities(pop_reg, mining_reg, clock, ctx);
        spawn_hills_cities(pop_reg, mining_reg, clock, ctx);
        spawn_city_states(pop_reg, mining_reg, clock, ctx);
        distribute_resource_mines(mining_reg, clock, ctx);
    }

    fun sc(pop: &mut PopulationRegistry, m: &mut MiningRegistry, q: u64, r: u64, n: vector<u8>, ct: u8, ctype: u8, clock: &Clock, ctx: &mut TxContext) {
        altriuxpopulation::spawn_city_with_type(pop, q, r, n, @0x0, ct, ctype, ctx);
        let mq = if (q < 419) { q + 1 } else { q - 1 };
        altriuxmining::spawn_mine(m, mq, r, altriuxmining::id_mine_type_hierro(), altriuxmining::reserve_iron(), @0x0, clock, ctx);
    }

